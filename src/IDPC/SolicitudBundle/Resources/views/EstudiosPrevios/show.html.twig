{% extends '::base.html.twig' %}

{% block body -%}
    <h1>EstudiosPrevios</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Objeto</th>
                <td>{{ entity.objeto }}</td>
            </tr>
            <tr>
                <th>Valorcontrato</th>
                <td>{{ entity.valorContrato }}</td>
            </tr>
            <tr>
                <th>Plazomeses</th>
                <td>{{ entity.plazoMeses }}</td>
            </tr>
            <tr>
                <th>Plazodias</th>
                <td>{{ entity.plazoDias }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Update_at</th>
                <td>{{ entity.updateat|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('estudiosprevios') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('estudiosprevios_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
